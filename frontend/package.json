{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fullcalendar/daygrid": "^6.1.11",
    "@fullcalendar/interaction": "^6.1.11",
    "@fullcalendar/react": "^6.1.11",
    "@fullcalendar/timegrid": "^6.1.11",
    "@hello-pangea/dnd": "^16.6.0",
    "@mui/icons-material": "^5.15.20",
    "@mui/material": "^5.15.20",
    "@mui/x-date-pickers": "^7.7.0",
    "@tanstack/react-query": "^5.45.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.7.2",
    "cronstrue": "^2.49.0",
    "date-fns": "^3.6.0",
    "framer-motion": "^11.2.10",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.51.5",
    "react-hot-toast": "^2.4.1",
    "react-joyride": "^2.8.2",
    "react-router-dom": "^6.23.1",
    "react-scripts": "5.0.1",
    "reactflow": "^11.11.3",
    "recharts": "^2.12.7",
    "web-vitals": "^2.1.4",
    "zustand": "^4.5.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "start-backend": "cd ../backend && poetry run uvicorn app.main:app --reload",
    "start-celery": "cd ../backend && poetry run celery -A app.celery_worker.celery_app worker --loglevel=info -P gevent",
    "services:up": "cd ../ && docker-compose up",
    "services:down": "cd ../ && docker-compose down --remove-orphans",
    "dev": "concurrently --kill-others --names \"SERVICES,BACKEND,CELERY,FRONTEND\" -c \"auto\" \"npm:services:up\" \"npm:start-backend\" \"npm:start-celery\" \"npm:start\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/hoist-non-react-statics": "^3.3.5",
    "concurrently": "^8.2.2"
  }
}
